<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.audio.song365" name="Song365" version="1.0.11" provider-name="PIkarus">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.xbmcswift2" version="2.4.0"/>
        <import addon="script.module.requests" version="1.1.0"/>
        <import addon="script.module.mutagen"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <language></language>
        <summary>ermöglicht den Zugriff auf Song365.cc</summary>
        <description>Zugriff auf tausende von MP3 Dateien
- Suche nach Künstler
- Suche nach Album
- Suche nach Titel in Album
- Suche nach Titel
- Künstler von A-Z
- Populäre Künstler
- Populäre Alben
- Populäre Titel
	</description>
	<website>https://pikarus0.github.io/Song365/</website>
    </extension>
</addon>
